{
  "name": "Ryry's Bookmarks",
  "description": "Chrome extension that helps organize user favorites.",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons":{
    "16": "./src/assets/ryry-icon.png",
    "48": "./src/assets/ryry-icon.png",
    "128": "./src/assets/ryry-icon.png"
  },
  "action": {
    "default_title": "Ryry's Bookmarks",
    "default_popup": "index.html"
  },

  "host_permissions":[
    "https://developer.chrome.com/*"
  ],

  "permissions":[
    "activeTab", "tabs", "tabGroups", "history"
  ]

}

  